{
	"manifest_version": 2,
	"name": "search_rutracker",
	"version": "0.1",
	"description": "Search Rutracker",

	"icons": {
		"48": "icons/icon.png",
		"96": "icons/icon@2x.png"
	},

	"chrome_settings_overrides": {
	  "search_provider": {
		"name": "Rutracker (by date)",
		"search_url": "https://rutracker.org/forum/tracker.php?nm={searchTerms}",
		"keyword": "r",
		"favicon_url": "https://rutracker.org/favicon.ico"
	  }
	}

}
